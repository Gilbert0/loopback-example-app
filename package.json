    "loopback-connector-rest": "git+ssh://git@github.com/strongloop/loopback-connector-rest.git",
    "request": "~2.21.0",
    "strong-task-emitter": "~0.0.4",
    "rc": "~0.3.0"
  },
  "optionalDependencies": {
